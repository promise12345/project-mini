/* pages/index/index.wxss */
.index_swiper swiper {
  width: 750rpx;
  height: 417rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_nav {
  display: flex;
}
.index_nav image {
  width: 35%;
  position: relative;
  left: 32%;
  justify-content: center;
}
.index_nav navigator {
  flex: 1;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-content: center;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_first {
  overflow: hidden;
}
.index_floor .floor_group .floor_first navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor_group .floor_first navigator:nth-last-child(-n+4) {
  height: 16.665vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_first navigator:nth-child(2),
.index_floor .floor_group .floor_first navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_first navigator image {
  width: 100%;
  height: 100%;
}
